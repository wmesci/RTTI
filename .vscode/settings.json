    "C_Cpp.commentContinuationPatterns": [
        "/// ", 
        "/**", 
        "// "
    ], 
    "clangTidy.extraCompilerArgs": [
        "-Weverything", 
        "-std=c++20"
    ], 
    "C_Cpp.default.includePath": [
        "Library/imgui"
    ],
    "C_Cpp.default.defines": [
        "WIN",
        "x64"
